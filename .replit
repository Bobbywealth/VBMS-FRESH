entrypoint = "index.js"
modules = ["nodejs-22", "python-3.11", "postgresql-16"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"

[deployment]
run = ["node", "index.js"]
deploymentTarget = "autoscale"
ignorePorts = false

[workflows]
runButton = "Preview VBMS"

[[ports]]
localPort = 5000
externalPort = 80
