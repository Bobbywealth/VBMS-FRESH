{
  "name": "vbms-backend",
  "version": "1.0.0",
  "description": "VBMS - Video Business Management System Backend API",
  "main": "server.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "node run-tests.js",
    "test:unit": "node tests/unit-tests.js",
    "test:integration": "node tests/test-suite.js",
    "lint": "echo \"No linting configured yet\"",
    "build": "echo \"Build completed\"",
    "deploy": "npm run build && npm run start",
    "health-check": "node -e \"console.log('Health check passed')\"",
    "db-seed": "node scripts/seed-database.js",
    "db-migrate": "node scripts/migrate-database.js",
    "create-admin": "node create-test-admin.js",
    "setup-stripe": "node setup-stripe-products.js",
    "logs": "tail -f logs/vbms.log || echo 'No log file found'",
    "pm2-start": "pm2 start ecosystem.config.js --env production",
    "pm2-stop": "pm2 stop vbms-backend",
    "pm2-restart": "pm2 restart vbms-backend",
    "pm2-logs": "pm2 logs vbms-backend",
    "backup:db": "node scripts/backup-database.js",
    "backup:files": "node scripts/backup-files.js",
    "backup:list": "node scripts/backup-database.js list && node scripts/backup-files.js list",
    "pre-launch": "node scripts/pre-launch-check.js",
    "fix-links": "node scripts/fix-html-links.js",
    "check-routes": "node scripts/check-duplicate-routes.js",
    "fix-theme": "node scripts/ensure-theme-manager.js",
    "fix-navigation": "node scripts/fix-sidebar-navigation.js"
  },
  "keywords": [
    "vbms",
    "business-management",
    "video-surveillance",
    "nodejs",
    "express",
    "mongodb",
    "stripe",
    "api"
  ],
  "author": "VBMS Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "imap": "^0.8.19",
    "jsonwebtoken": "^9.0.0",
    "mailparser": "^3.7.4",
    "mongoose": "^6.8.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.6.7",
    "nodemailer": "^7.0.5",
    "openai": "^4.28.0",
    "stripe": "^12.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
